    int[] result = new int[nums.length];
    int i = 0, j = nums.length - 1, index = (a >= 0) ? nums.length - 1 : 0;
    
    while (i <= j) {
        int left = nums[i] * nums[i] * a + nums[i] * b + c;
        int right = nums[j] * nums[j] * a + nums[j] * b + c;
        
