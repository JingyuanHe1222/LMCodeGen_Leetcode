public double[] medianSlidingWindow(int[] nums, int k) {
    TreeMap<Integer, Integer> window = new TreeMap<>();
    int n = nums.length;
    double[] result = new double[n - k + 1];

    for (int i = 0; i < k; i++) {
        window.put(nums[i], window.getOrDefault(nums[i], 0) + 1);
