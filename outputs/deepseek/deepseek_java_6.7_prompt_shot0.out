def minSkips(self, dist: List[int], speed: int, hoursBefore: int) -> int:
    pq, total = [], 0
    for d in dist:
        t = (d + speed - 1) // speed
        total += t
        if pq and pq[0] < t:
            total -= heapq.heappop(pq)
            heapq.heappush(pq, t)
        else:
            pq.append(t)
        if total > hoursBefore:
            return len(pq)
    return -1 if total > hoursBefore else len(pq)
