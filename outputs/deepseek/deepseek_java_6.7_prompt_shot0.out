class Solution:
    def chalkReplacer(self, chalk: List[int], k: int) -> int:
        n = len(chalk)
        prefix_sum = [0] * n
        prefix_sum[0] = chalk[0]
        for i in range(1, n):
            prefix_sum[i] = prefix_sum[i - 1] + chalk[i]
            if prefix_sum[i] > k:
                return i
        k %= prefix_sum[-1]
        left, right = 0, n - 1
        while left < right:
            mid = (left + right + 1) // 2
            if prefix_sum[mid] <= k:
                left = mid
            else:
                right = mid - 1
        return left + 1
