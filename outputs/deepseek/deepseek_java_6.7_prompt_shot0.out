class Solution:
    def countPairs(self, n: int, edges: List[List[int]], queries: List[int]) -> List[int]:
        degree = [0] * (n + 1)
        edge_count = defaultdict(int)
        for u, v in edges:
            if u > v:
                u, v = v, u
            degree[u] += 1
            degree[v] += 1
            edge_count[(u, v)] += 1
        
        sorted_degrees = sorted((d, i) for i, d in enumerate(degree))
        prefix_sum = [0] * (n + 1)
