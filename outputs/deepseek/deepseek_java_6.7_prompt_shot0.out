class Solution:
    def removeZeroSumSublists(self, head: ListNode) -> ListNode:
        dummy = ListNode(0)
        dummy.next = head
        prefix = 0
        seen = {0: dummy}
        
        while head:
            prefix += head.val
            if prefix in seen:
                node = seen[prefix]
                tmp = node.next
                tmpPrefix = prefix + tmp.val
                while tmp != head:
                    del seen[tmpPrefix]
                    tmp = tmp.next
                    tmpPrefix += tmp.val
                node.next = head.next
            else:
                seen[prefix] = head
            head = head.next
                
        return dummy.next
```
