from collections import deque
class Solution:
    def reverseOddLevels(self, root):
        stack = []
        queue = deque([(root, 0)])
        
        while queue:
            node, level = queue.popleft()
            
            if node.left:
                queue.append((node.left, level + 1))
                queue.append((node.right, level + 1))
                
            if level % 2 == 1:
                stack.append(node.val)
                
            elif stack:
                node.val = stack.pop()
                
        return root
```

