public int maxProfit(int[] inventory, int orders) {
    long mod = 1000000007;
    Integer[] inv = Arrays.stream(inventory).boxed().toArray(Integer[]::new);
    Arrays.sort(inv, Collections.reverseOrder());
    List<Integer> invList = new ArrayList<>(Arrays.asList(inv));
