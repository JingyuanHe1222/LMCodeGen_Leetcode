private void solve(List<List<String>> result, List<Integer> current, int n, int col) {
    if (col == n) {
        result.add(generateBoard(current, n));
        return;
    }
    for (int row = 0; row < n; row++) {
        if(isSafe(current, row, col)) {
            current.add(row);
            solve(result, current, n, col + 1);
            current.remove(col);
        }
    }
