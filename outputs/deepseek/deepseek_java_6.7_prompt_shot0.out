public int maximumRequests(int n, List<List<Integer>> requests) {
    int req_size = requests.size();
    int max_requests = 0;

    for (int i = 0; i < (1 << req_size); ++i) {
        int[] net_transfers = new int[n];
        int count = 0;

        for (int j = 0; j < req_size; ++j) {
