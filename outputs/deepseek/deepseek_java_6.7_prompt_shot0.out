private boolean isSafe(List<Integer> current, int row, int col) {
    for (int i = 0; i < col; i++) {
        int prevRow = current.get(i);
        if (prevRow == row || (Math.abs(col - i) == Math.abs(row - prevRow))) {
            return false;
        }
    }
    return true;
}

