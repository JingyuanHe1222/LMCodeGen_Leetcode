public class Solution {
    private static final int MOD = 1_000_000_007;

    private static int mul(int a, int b) {
        return (int)(((long) a * b) % MOD);
    }

    private static int powMod(int a, int b) {
        int res = 1;
        while (b > 0) {
            if ((b & 1) == 1) {
                res = mul(res, a);
            }
            a = mul(a, a);
            b >>= 1;
        }
        return res;
