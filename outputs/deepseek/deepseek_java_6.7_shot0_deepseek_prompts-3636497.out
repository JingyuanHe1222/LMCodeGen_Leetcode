private void helper(int[] nums, int index, List<List<Integer>> results) {
    if (index == nums.length - 1) {
        List<Integer> tempList = new ArrayList<>();
        for (int n : nums) {
            tempList.add(n);
        }
        results.add(tempList);
        return;
