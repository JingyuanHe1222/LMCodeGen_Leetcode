class Solution {
    public int longestSubarray(int[] nums) {
        int n = nums.length;
        int maxLen = 0;
        int zeroCount = 0;
        int left = 0, right = 0;

        while (right < n) {
            if (nums[right] == 0) {
                zeroCount++;
            }
            while (zeroCount > 1) {
                if (nums[left] == 0) {
                    zeroCount--;
                }
                left++;
            }
