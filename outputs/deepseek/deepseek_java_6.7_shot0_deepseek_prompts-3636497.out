class Solution {
    public int maxTurbulenceSize(int[] arr) {
        int n = arr.length;
        int res = 1, cnt = 1;
        boolean flag = true; // true means '>', false means '<'
        for (int i = 1; i < n; ++i) {
            if (arr[i] == arr[i - 1]) {
                flag = true;
                cnt = 1;
            } else if ((flag && arr[i] < arr[i - 1]) || (!flag && arr[i] > arr[i - 1])) {
                flag = !flag;
                ++cnt;
                res = Math.max(res, cnt);
            } else {
                flag = true;
                cnt = 2;
            }
        }
        return res;
    }
}
