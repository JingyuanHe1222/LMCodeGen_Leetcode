import java.util.*;

class Solution {
    public int minStoneSum(int[] piles, int k) {
        PriorityQueue<Integer> maxHeap = new PriorityQueue<>((a, b) -> b - a);
        int sum = 0;
        for (int i : piles) {
            maxHeap.add(i);
            sum += i;
        }
        
        while (k-- > 0) {
            int curr = maxHeap.poll();
            int remove = curr / 2;
            sum -= remove;
            maxHeap.add(curr - remove);
        }
        
        return sum;
    }
}
```
