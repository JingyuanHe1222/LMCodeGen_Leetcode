                map.put(cLeft, map.get(cLeft) - 1);
                
                if (map.get(cLeft) == 0) {
                    map.remove(cLeft);
                }
                
                left++;
            }
            
            maxLen = Math.max(maxLen, right - left + 1);
            right++;
        }
        
        return maxLen;
    }
}
