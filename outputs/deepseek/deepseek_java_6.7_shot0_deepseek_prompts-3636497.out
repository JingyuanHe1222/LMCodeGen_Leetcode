public int countPartitions(int[] nums, int k) {
    long total_sum = 0;
    int n = nums.length;
    int mod = 1000000007;

    for (int num : nums) {
        total_sum += num;
    }

    if (total_sum < (long) k * 2) {
        return 0;
    }

    long[] dp = new long[n + 1];
    dp[0] = 1;

    for (int i = 1; i <= k - 1; i++) {
        for (int j = n - 1; j >= 1; j--) {
            dp[j] = (dp[j] * j + dp[j - 1]) % mod;
        }
    }

    long result = 0;

    for (int i = 0; i < n - 1; i++) {
        total_sum -= nums[i];
        if (total_sum >= k) {
            result = (result + dp[i + 1]) % mod;
        }
    }

    return (int) result;
