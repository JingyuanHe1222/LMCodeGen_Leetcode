                if (char_count.get(s.charAt(l)) == 0) {
                    char_count.remove(s.charAt(l));
                }
                l++;
                cur_len--;
            }
            cur_len++;
        }
        max_len = Math.max(max_len, cur_len);
        r++;
    }
    return max_len;
}

