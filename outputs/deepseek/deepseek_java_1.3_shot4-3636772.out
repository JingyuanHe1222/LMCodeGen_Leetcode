double helper(int n, int k, int row, int column, double[][][] memo) {
    if (row < 0 || row >= n || column < 0 || column >= n) return 0.0;
    if (k == 0) return 1.0;
    if (memo[row][column][k] >= 0) return memo[row][column][k];

