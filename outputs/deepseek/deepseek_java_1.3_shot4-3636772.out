    private boolean searchHelper(TrieNode node, String word, int index) {
        if (index == word.length()) {
            return node.is_word;
        }

        if (word.charAt(index) == '.') {
            for (int i = 0; i < 26; i++) {
                if (node.children[i] != null && searchHelper(node.children[i], word, index + 1)) {
                    return true;
