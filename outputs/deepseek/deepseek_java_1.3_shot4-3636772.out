public String parseTernary(String expression) {
    Stack<Character> stk = new Stack<>();
    for (int i = expression.length() - 1; i >= 0; --i) {
        char c = expression.charAt(i);
        if (!stk.isEmpty() && stk.peek() == '?') {
            stk.pop(); // Pop '?'
            char first = stk.pop();
            stk.pop(); // Pop ':'
            char second = stk.pop();
            stk.push(c == 'T' ? first : second);
        } else {
            stk.push(c);
        }
    }
