        boolean lockedDescendant = false;
        for(int child : children.get(node)) {
            if(locks[child] != -1) {
                lockedDescendant = true;
                locks[child] = -1;
            }
            lockedDescendant |= hasLockedDescendants(child);
        }
        return lockedDescendant;
    }
