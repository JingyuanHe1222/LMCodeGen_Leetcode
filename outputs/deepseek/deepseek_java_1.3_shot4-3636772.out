public void reorderList(ListNode head) {
    if (head == null || head.next == null || head.next.next == null) return;

    // Find the middle node
    ListNode slow = head;
    ListNode fast = head;
    while (fast.next != null && fast.next.next != null) {
        slow = slow.next;
        fast = fast.next.next;
    }

    // Reverse the second half
    ListNode prev = null;
    ListNode curr = slow.next;
    ListNode next;
    while (curr != null) {
        next = curr.next;
        curr.next = prev;
        prev = curr;
        curr = next;
    }
    slow.next = null;

    // Merge two halves
    ListNode first = head;
    ListNode second = prev;
    while (second != null) {
        next = first.next;
        first.next = second;
        first = second;
        second = next;
    }
}

