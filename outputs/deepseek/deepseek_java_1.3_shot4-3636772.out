                int[] up = i > 0 ? dp[i - 1][j] : new int[]{-1, 0};
                int[] left = j > 0 ? dp[i][j - 1] : new int[]{-1, 0};
                int[] diag = i > 0 && j > 0 ? dp[i - 1][j - 1] : new int[]{-1, 0};

