    public Solution(int m, int n) {
        this.m = m;
        this.n = n;
        total = m * n;
        matrix = new int[m][n];
        random = new Random();
    }

    public int[] flip() {
        int idx = random.nextInt(total);
        total--;

        for (int i = 0; i < m; i++) {
            for (int j = 0; j < n; j++) {
                if (matrix[i][j] == 0) {
                    if (idx == 0) {
                        matrix[i][j] = 1;
                        return new int[] { i, j };
                    }
                    idx--;
                }
            }
        }
        return new int[] { -1, -1 }; // This line should never be reached
    }

    public void reset() {
        total = m * n;
        for (int i = 0; i < m; i++) {
            for (int j = 0; j < n; j++) {
                matrix[i][j] = 0;
            }
        }
    }
